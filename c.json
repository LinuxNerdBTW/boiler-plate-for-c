"cmain":{
	"prefix": "boilerplate for C main body",
	"body": [
		"#include <stdio.h>",
		"int main()",
		"{",
		"	$1",
		"	return 0;",
		"}"
	],
	"description": "Log output to console"
}

"cfor": {
	"prefix": "boilerplate for C for loop",
	"body": [
		"#include <stdio.h>",
		"int main()",
		"{",
		"for ( $1)",
		"	{",
		"		/* Statement here */",
		"	}",
		"	return 0;",
		"}"
	],
	"description": "Log output to console"
}



